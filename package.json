{
  "name": "pg-iles",
  "version": "0.0.0",
  "homepage": "https://pg-iles-css.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-iles.git"
  },
  "scripts": {
    "dev": "iles dev",
    "build": "iles build",
    "preview": "iles preview --open --port 5050",
    "now": "npm run build && npm run preview",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write . '!public'"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "pinia": "^2.0.35"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.64",
    "@nuxt/eslint-config": "^0.1.1",
    "@pinegrow/iles-module": "^3.0.0-beta.134",
    "@unocss/preset-icons": "^0.51.12",
    "@vue/devtools": "^6.5.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.6.0",
    "iles": "^0.9.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "unocss": "^0.51.12",
    "vite-plugin-vue-devtools": "^0.0.20",
    "vue-tsc": "^1.6.4"
  },
  "web-types": "web-types.json"
}
